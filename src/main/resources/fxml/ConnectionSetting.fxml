<VBox prefHeight="480.0" prefWidth="450.0" styleClass="backPane" stylesheets="@../css/global.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dtflys.redistart.controller.ConnectionSettingController">
   <children>
      <JFXTabPane fx:id="connSettingTabView" prefHeight="410.0" styleClass="connSettingsTabView">
         <tabs>
            <Tab fx:id="tabConn" text="连接">
