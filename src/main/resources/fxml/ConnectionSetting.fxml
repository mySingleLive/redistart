                           <HBox GridPane.columnIndex="1" GridPane.rowIndex="2">
                              <children>
                                 <CustomTextField fx:id="txPort" alignment="CENTER" prefColumnCount="0" prefWidth="60.0" promptText="(必填) 端口号" text="6379" />
