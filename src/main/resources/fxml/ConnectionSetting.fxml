                                 <PasswordField fx:id="txAuth" minWidth="0.0" prefWidth="0.0" promptText="(可选) Redis服务验证密码" visible="false" />
                                 <TextField fx:id="txAuthVisible" prefWidth="260.0" promptText="(可选) Redis服务验证密码" />
                                 <Region prefWidth="10.0" />
                                 <FontIcon fx:id="iconPwdVisibility" iconColor="WHITE" iconLiteral="icm-eye" iconSize="18" onMouseClicked="#onPwdVisibilityClick" selectionEnd="1">
