            <JFXButton buttonType="RAISED" onAction="#onOkAction" text="确定" />
            <Region prefWidth="10.0" />
            <JFXButton buttonType="RAISED" cancelButton="true" onAction="#onCancelAction" text="取消" />
            <Region prefWidth="20.0" />
