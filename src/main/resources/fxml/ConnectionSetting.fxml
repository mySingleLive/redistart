                       </rowConstraints>
                        <children>
                           <Label text="Redis地址" GridPane.rowIndex="1" />
                           <Label text="Redis端口" GridPane.rowIndex="2" />
                           <Label text="名称" />
                           <CustomTextField fx:id="txName" promptText="(必填) 连接名称" GridPane.columnIndex="1" />
